{
    "name": "dized/repairable-client-php",
    "description": "Repairable PHP Client",
    "version": "1.0.0",
    "keywords": ["dized", "repairable", "api", "client", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DiZed Team",
            "email": "d1ma.7.zed@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "DiZed\\RepairableClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DiZed\\RepairableClient\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
